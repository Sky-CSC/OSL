    @if (EndGamePage.textValueOverlayView3.BackgroundColor)
    {
        tempsBackgroundColor = formatingData.BackgroundColor;
        //tempsBackgroundColor = "";
    }
    else
    {
        tempsBackgroundColor = "";
    }
}

