{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "../OSL-Client/"
        }
      ],
      "dest": "apiclient",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "../OSL-Server/"
        }
      ],
      "dest": "apiserver",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "apiclient/**.yml",
          "apiclient/index.md",
          "apiserver/**.yml",
          "apiserver/index.md"
        ]
      },
      {
        "files": [
          "clientdoc/**.md",
          "clientdoc/**/toc.yml",
          "serverdoc/**.md",
          "serverdoc/**/toc.yml",

          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "picture/**",
          "misc/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "statictoc",
      "darkfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}