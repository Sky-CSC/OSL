{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "../OSL-Client/"
        }
      ],
      "dest": "apiclient",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "../OSL-Server/"
        }
      ],
      "dest": "apiserver",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "apiclient/**.yml",
          "apiclient/index.md",
          "apiserver/**.yml",
          "apiserver/index.md"
        ]
      },
      {
        "files": [
          "clientdoc/**.md",
          "clientdoc/**/toc.yml",
          "serverdoc/**.md",
          "serverdoc/**/toc.yml",

          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "picture/**",
          "misc/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    //"template": [
    //  "statictoc",
    //  //"darkfx",
    //  "SingulinkFX"
    //],
    "globalMetadata": {
      "_appTitle": "OSL",
      "_appName": "Overlay Spectator Live",
      "_appFaviconPath": "picture/Icone_Feu.png",
      "_appLogoPath": "picture/Icone_Feu.png",
      "_appFooter": "<strong><a href='https://github.com/Sky-CSC/OSL'>Sky csc</a> </strong>",
      "_copyrightFooter": "Â© Sky csc. All rights reserved.",
      "_enableSearch": true,
      "_disableSideFilter": false,
      "_enableNewTab": true,
      "_disableContribution": false,
      "_disableBreadcrumb": false
    },
    "template": [ "default", "templates/singulinkfx", "plugins/memberpage.2.59.0/content" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}